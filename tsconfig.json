{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
